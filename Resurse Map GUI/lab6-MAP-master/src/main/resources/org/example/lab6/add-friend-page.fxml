<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.lab6.controller.AddFriendPageController">
   <children>
      <VBox prefHeight="500.0" prefWidth="500.0">
         <children>
            <Label prefHeight="50.0" prefWidth="500.0" text="Search user: " />
            <TextField />
            <TableView fx:id="tableViewUsers" prefHeight="356.0" prefWidth="500.0">
              <columns>
                <TableColumn fx:id="tableColumnFullName" prefWidth="243.0" text="Full name" />
                <TableColumn fx:id="tableColumnUsername" prefWidth="256.0" text="Username" />
              </columns>
            </TableView>
            <Separator opacity="0.0" prefHeight="29.0" prefWidth="500.0" />
            <HBox prefHeight="74.0" prefWidth="500.0">
               <children>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="89.0" prefWidth="59.0" />
                  <Button fx:id="buttonRequest" mnemonicParsing="false" onAction="#handleButtonRequestClicked" prefHeight="42.0" prefWidth="354.0" text="Send a request!" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
